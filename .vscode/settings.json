{
	"files.exclude": {
		"**/*.tsx": true,
		"**/*.css": true,
		"**/*.json": true,
		"**/ios": true,
		//hide a file with name pubspec.lock
		"**/pubspec.lock": true,
		//hide .metadata
		"**/.metadata": true,
		//hide .gitignore
		"**/.gitignore": true,
		//Hide all of: algorithms.iml, .flutter-plugins, .flutter-plugins-dependencies, windows, macos, web, .dart_tool, .github, linux, android
		"**/propositional_analyzer.iml": true,
		"**/.flutter-plugins": true,
		"**/.flutter-plugins-dependencies": true,
		"**/windows": true,
		"**/macos": true,
		"**/web": true,
		"**/.dart_tool": true,
		"**/.github": false,
		"**/linux": true,
		"**/.idea": true,
		"**/build": true,
		"**/android": true,
		//hide readme
		// hide .vscode
		"**/.vscode": true,
		//hide analysis_options.yaml
		"**/analysis_options.yaml": true,
		"**/devtools_options.yaml": true,
		//also assets
		"**/assets": falsse
	}
}
